    audio_play_sound(choose(snd_pick_up0, snd_pick_up1, snd_pick_up2), 10, false);
    var oldHP = player.currHP;
    player.currHP = min(player.currHP + currencyGain, player.maxHP);
    global.currency[0] += max(currencyGain - (player.currHP - oldHP), 0); 
    instance_destroy(); // TODO
}else{
    player = instance_find(obj_player, 0);
    if(player != noone){
        var dir = point_direction(x, y, player.x, player.y);
        var dist = point_distance(x, y, player.x, player.y);
        if(dist &lt; 64){
            hspeed = lengthdir_x(2.0 - dist/64.0, dir);
            vspeed = lengthdir_y(2.0 - dist/64.0, dir);
        }
    }
}
