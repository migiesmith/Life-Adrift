var player = instance_find(obj_player, 0);
if(player != noone){
    var dist = point_distance(x, y, player.x, player.y);
    if(dist &lt; 32 * 6 &amp;&amp; dist &gt; minDist &amp;&amp; !isBusy){
        if(isPathing){
            if(pathPos == path_get_number(path)){
                isPathing = false;
                pathPos = 1;
            }else{
                pathEndX = path_get_point_x(path, 1);
                pathEndY = path_get_point_y(path, 1);
                
                if(pathEndX &gt; x){
                    image_xscale = 1;
                }else if(pathEndX &lt; x){
                    image_xscale = -1;
                }
                STATE = WALKING;
                //if(lastX == x &amp;&amp; lastY == y){
