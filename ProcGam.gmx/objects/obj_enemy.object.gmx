                mp_potential_step_object(pathEndX, pathEndY, moveSpeed, avoidType);
                //}
            }
            pathPos++;
        }else{
            isPathing = canGetPath(x, y, player.x, player.y);
            pathPos = 1;
        }
    }else{
        isPathing = false;
        if(!isBusy)
            STATE = IDLE;
        pathPos = 1;
    }
